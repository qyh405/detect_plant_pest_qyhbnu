{
    "api": {
        "api_key": "" , 
        "model": "qwen-vl-max" , 
        "api_url": "https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions",
        "max_batch_size": 10
    },
    "paths": {
        "result_dir": "result_plant_pest",
        "cache_dir": "${result_dir}/.cache",
        "log_dir": "${result_dir}/.log",
        "data_dir": "small_test"
    },
    "image": {
        "resized_image_size": [256, 256],
        "contrast_enhance_ratio": 1.3,
        "image_quality": 95
    },
    "training": {
        "target_diseases": ["稻瘟病", "细菌性枯萎病", "水稻东格鲁病毒病", "褐斑病"],
        "retrain_threshold": 80,
        "sample_size": 200,
        "test_size": 0.05,
        "random_state": 42,
        "force_retrain": true
    },
    "prediction": {
        "low_confidence_threshold": 70,
        "max_tokens": 4096,
        "prediction_max_tokens": 1024
    }
}
